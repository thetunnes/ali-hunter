{
  "manifest_version": 3,
  "name": "Ali Hunter",
  "description": "Auxilia na compra em lojas shopify",
  "author": "Tunnes",
  "icons": {
    "32": "alihunter-icon.jpg"
  },
  "action": {
    "default_title": "Ali Hunter"
  },
  "permissions": ["tabs"],
